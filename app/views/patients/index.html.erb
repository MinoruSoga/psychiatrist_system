<div class="container">

  <h1>患者一覧</h1>
  <%= link_to '追加', new_patient_path %>
  <table class="table">
  <thead>
    <tr>
      <th>患者名</th>
      <th>年齢</th>
      <th>性別</th>
      <th>生年月日</th>
    </tr>
  </thead>
  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.patient_name %></td>
        <td><%= patient.age %></td>
        <td><%= patient.gender %></td>
        <td><%= patient.birthday %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
</div>
